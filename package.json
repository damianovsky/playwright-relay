{
  "name": "playwright-relay",
  "version": "1.0.1-beta.4",
  "description": "Pass data between Playwright tests using @depends annotations",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/damianovsky/playwright-relay"
  },
  "bugs": {
    "url": "https://github.com/damianovsky/playwright-relay/issues"
  },
  "homepage": "https://github.com/damianovsky/playwright-relay#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build"
  },
  "keywords": [
    "playwright",
    "testing",
    "test-dependencies",
    "relay",
    "depends",
    "e2e",
    "api-testing"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@playwright/test": ">=1.30.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}
